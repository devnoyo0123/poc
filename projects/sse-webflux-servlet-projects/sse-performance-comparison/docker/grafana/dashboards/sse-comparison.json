{
  "dashboard": {
    "title": "SSE Performance Comparison: Servlet vs WebFlux",
    "tags": ["sse", "performance", "webflux", "servlet"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Active SSE Connections",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sse_servlet_connections_active",
            "legendFormat": "Servlet (Tomcat)",
            "refId": "A"
          },
          {
            "expr": "sse_webflux_connections_active",
            "legendFormat": "WebFlux (Netty)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 2,
        "title": "JVM Thread Count (CRITICAL METRIC)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "jvm_threads_live_threads{application=\"servlet-stack\"}",
            "legendFormat": "Servlet Threads",
            "refId": "A"
          },
          {
            "expr": "jvm_threads_live_threads{application=\"webflux-stack\"}",
            "legendFormat": "WebFlux Threads",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 200}
              ]
            }
          }
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [200], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ]
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 3,
        "title": "JVM Heap Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{application=\"servlet-stack\",area=\"heap\"}",
            "legendFormat": "Servlet Heap",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_used_bytes{application=\"webflux-stack\",area=\"heap\"}",
            "legendFormat": "WebFlux Heap",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 4,
        "title": "JVM Non-Heap Memory Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{application=\"servlet-stack\",area=\"nonheap\"}",
            "legendFormat": "Servlet Non-Heap",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_used_bytes{application=\"webflux-stack\",area=\"nonheap\"}",
            "legendFormat": "WebFlux Non-Heap",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 5,
        "title": "Process CPU Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "system_cpu_usage{application=\"servlet-stack\"}",
            "legendFormat": "Servlet CPU",
            "refId": "A"
          },
          {
            "expr": "system_cpu_usage{application=\"webflux-stack\"}",
            "legendFormat": "WebFlux CPU",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 6,
        "title": "Prices Generated per Second (Throughput)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(sse_prices_generated_total[1m])",
            "legendFormat": "Prices/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 7,
        "title": "Active SSE Subscribers",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sse_subscribers_active",
            "legendFormat": "Active Subscribers",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 8,
        "title": "Total SSE Connections",
        "type": "stat",
        "gridPos": {"x": 12, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sse_servlet_connections_total",
            "legendFormat": "Servlet",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5000},
                {"color": "red", "value": 9000}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "GC Pause Time",
        "type": "graph",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_sum[1m])",
            "legendFormat": "Servlet GC",
            "refId": "A"
          },
          {
            "expr": "rate(jvm_gc_pause_seconds_sum{application=\"webflux-stack\"}[1m])",
            "legendFormat": "WebFlux GC",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 10,
        "title": "Performance Summary",
        "type": "stat",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 4},
        "targets": [
          {
            "expr": "jvm_threads_live_threads{application=\"servlet-stack\"}",
            "legendFormat": "Servlet Threads",
            "refId": "A"
          },
          {
            "expr": "jvm_threads_live_threads{application=\"webflux-stack\"}",
            "legendFormat": "WebFlux Threads",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      }
    ]
  }
}
